<h1>Posts#new</h1>

<%= form_for @post do |f|%>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :content %>
<%= f.text_field :content %>
<%= f.label :likes %>
<%= f.number_field :likes %>
<%= f.collection_select :blogger_id, Blogger.all, :id, :name %>
<%= f.collection_select :destination_id, Destination.all, :id, :name %>
<%= f.submit %>
<% end %>


<p>Find me in app/views/posts/new.html.erb</p>
